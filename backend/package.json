  "main": "./src/server.js",
  "scripts": {
    "start": "yarn && lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- nodemon ./src/app.js --exec babel-node -r './node_modules/dotenv/config' -e js",
    "quick-start": "QUICKSTART=true bash start.sh",
    "start-docker": "bash start-docker.sh",
    "launch": "nodemon ./src/app.js --exec babel-node -e js",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "lookenv -- node ./app.js",
    "serve-production": "lookenv -- node ./app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json",
    "deploy": "bash deploy/init.sh"
  },
