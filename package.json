{
  "name": "innential_platform",
  "version": "0.2.8",
  "description": "Innential",
  "private": true,
  "license": "ISC",
  "author": "Waat <enrico@waat.eu>",
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-catch-binding": "^7.8.3",
    "concurrently": "^3.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "^1.2.0"
  },
  "scripts": {
    "cypress-run": "cypress run",
    "start-dev-apollo": "cd backend && yarn start",
    "start-dev-apollo-quick": "cd backend && yarn quick-start",
    "start-dev-apollo-docker": "cd backend && yarn start-docker",
    "start-dev-admin": "cd adminApp && yarn dev",
    "start-dev-user": "cd userApp && yarn dev",
    "lint-backend": "cd backend && yarn run lint",
    "lint-admin": "cd adminApp && yarn run lint",
    "lint-user": "cd userApp && yarn run lint",
    "lint": "concurrently 'yarn lint-user' 'yarn lint-admin' 'yarn lint-backend'",
    "lint-fix-backend": "cd backend && yarn run lint-fix",
    "lint-fix-admin": "cd adminApp && yarn run lint-fix",
    "lint-fix-user": "cd userApp && yarn run lint-fix",
    "lint-fix": "concurrently 'yarn lint-fix-user' 'yarn lint-fix-admin' 'yarn lint-fix-backend'",
    "install-dev-apollo": "cd backend && yarn install",
    "install-dev-admin": "cd adminApp && yarn install",
    "install-dev-user": "cd userApp && yarn install",
    "dev-apollo-react": "concurrently  --kill-others 'yarn start-dev-apollo' 'yarn start-dev-admin'",
    "dev": "concurrently  --kill-others 'yarn start-dev-apollo' 'yarn start-dev-admin' 'yarn start-dev-user'",
    "quickstart": "concurrently  --kill-others 'yarn start-dev-apollo-quick' 'yarn start-dev-admin' 'yarn start-dev-user'",
    "dev-docker": "concurrently  --kill-others 'yarn start-dev-apollo-docker' 'yarn start-dev-react'",
    "install-all-apollo-react": "yarn install && concurrently 'yarn install-dev-apollo' 'yarn install-dev-react'",
    "install-all": "yarn install && concurrently 'yarn install-dev-apollo' 'yarn install-dev-admin' 'yarn install-dev-user'",
    "start-apollo-react": "yarn run dev-apollo-react",
    "start": "yarn run dev",
    "start-nobrowser": "concurrently  --kill-others 'yarn start-dev-apollo' 'yarn start-dev-admin-nobrowser' 'yarn start-dev-user-nobrowser'",
    "start-docker": "yarn run dev-docker",
    "deploy-staging": "cd backend && yarn deploy staging-api && cd ../adminApp && yarn deploy staging-admin && cd ../userApp && yarn deploy staging-frontend && cd ../haproxy && yarn deploy staging-haproxy",
    "deploy-production": "cd backend && yarn deploy production-api && cd ../adminApp && yarn deploy production-admin && cd ../userApp && yarn deploy production-frontend && cd ../haproxy && yarn deploy production-haproxy",
    "uninstall": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "start-dev-admin-nobrowser": "cd adminApp && yarn start-nobrowser",
    "start-dev-user-nobrowser": "cd userApp && yarn start-nobrowser"
  },
  "dependencies": {
    "aws-sdk": "^2.866.0",
    "cypress": "^3.2.0",
    "ga-4-react": "^0.1.281",
    "object-visit": "^1.0.1",
    "pre-commit": "^1.2.2",
    "start-server-and-test": "^1.9.1"
  }
}
